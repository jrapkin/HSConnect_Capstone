@model IEnumerable<HSconnect.Models.Chart>
@{
    Layout = "~/Views/Shared/_Layout_SocialWorker.cshtml";
    ViewData["Title"] = "Resources";
}

<h1>Status</h1>

<table class ="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ServiceOffered.Address)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ServiceOffered.Category)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ServiceOffered.Provider.PhoneNumber)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ServiceOffered.Demographic)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ServiceOffered.Cost)
            </th>
           
        </tr>                     
    </thead>

    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ServiceOffered.Address)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ServiceOffered.Category)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ServiceOffered.Provider.PhoneNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ServiceOffered.Demographic)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ServiceOffered.Cost)
            </td>

        </tr>
        }
    </tbody>
</table>

