@model HSconnect.Models.PartnershipViewModel

@{
    ViewData["Title"] = "CreatePartnership";
    Layout = "~/Views/Shared/_Layout_Provider.cshtml";
}

<h1>CreatePartnership</h1>

<h4>Partnership</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="CreatePartnership">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="ManagedCareOrganizationSelectionId" class="control-label"></label>
                <select asp-for="ManagedCareOrganizationSelectionId" class ="form-control" asp-items='new SelectList(Model.ManagedCareOrganizations, "Id", "Name")'></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="DisplayPartnerships">Back to Partnerships</a>
</div>

